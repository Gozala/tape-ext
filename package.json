{
  "version": "0.2.0",
  "name": "tape-ext",
  "description": "A command line tool (inspired by tape) to test web extensions",
  "keywords": [
    "tape-ext",
    "typed",
    "flow"
  ],
  "main": "lib/tape-ext",
  "module": "src/tape-ext",
  "unpkg": "dist/tape-ext",
  "author": "Irakli Gozalishvili <rfobic@gmail.com> (http://jeditoolkit.com)",
  "repository": "https://github.com/Gozala/tape-ext",
  "license": "MIT",
  "bin": {
    "tape-ext": "src/index.js"
  },
  "dependencies": {
    "esm": "3.0.72",
    "fx-runner": "1.0.9",
    "geckodriver": "1.11.0",
    "glob": "7.1.2",
    "mz": "2.7.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "tap-finished": "0.0.1"
  },
  "devDependencies": {
    "alias-quokka-plugin": "^0.1.0",
    "blue-tape": "^1.0.0",
    "flow-bin": "^0.77.0",
    "lint-staged": "^7.2.0",
    "prettier": "^1.9.2"
  },
  "scripts": {
    "test": "npm run test:flow && npm run test:tape",
    "test:tape": "blue-tape 'test/**/*.js'",
    "test:flow": "flow check",
    "prepublishOnly": "npm test",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --parser flow --no-semi --write",
      "git add"
    ]
  },
  "quokka": {
    "alias": {
      "tape-ext": "src/tape-ext"
    },
    "pro": true,
    "plugins": [
      "alias-quokka-plugin"
    ]
  }
}
